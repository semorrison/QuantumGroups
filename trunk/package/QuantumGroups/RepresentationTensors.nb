  InitializationCell->True],

Cell[BoxData[
    \(QuantumTrace[
        RepresentationTensor[\[CapitalGamma]_, V_, \[Beta]_, V_, \[Beta]_, 
          matrices_]] := 
      Simplify[\[Sum]\+\(i = \
